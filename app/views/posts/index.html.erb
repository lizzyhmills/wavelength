<div class="container">
  <h1 class="text-center">My Feed</h1>

  <% @posts.each do |post| %>
    <div class="post <%= post.user == current_user ? "current-user-post" : "other-user-post" %>">
      <div class="post-top">
        <%= cl_image_tag post.user.photo.key, class: "avatar-bordered" %>
        <%= post.user.username %> <br>

      </div>

      <div class="song-info">
        <img src="<%= post.image_url%>" alt="" class="album-artwork">
       <%= post.song_name %> <br>
        <%= post.artist %> <br>
        <%= post.caption %> <br>
      </div>
      <%= link_to "edit post", edit_post_path(post) %>
    </div>
  <% end %>
  <% @myposts.each do |post| %>
    <div class="post <%= post.user == current_user ? "current-user-post" : "other-user-post" %>">
      <div class="post-top">
      <%= cl_image_tag post.user.photo.key, class: "avatar-bordered" %>
        <%= post.user.username %> <br>
      </div>
      <div class="song-info">
        <img src="<%= post.image_url%>" alt="" class="album-artwork">
        <em>Title:</em> <%= post.song_name %> <br>
        <em>Artist:</em> <%= post.artist %> <br>
        <em>Caption:</em> <%= post.caption %> <br>
      </div>
      <%= link_to "edit post", edit_post_path(post) %>
    </div>
    <% end %>

</div>
