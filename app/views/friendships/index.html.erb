<%# SUGGESTIONS %>
<div>
  <% @users.each do |user| %>
    <%= user.avatar_url %>
    <%= user.first_name %>
    <%=user.last_name %>
    <%= user.username %>
    <a href="#">add</a>
    <p>x</p>
  <% end %>
