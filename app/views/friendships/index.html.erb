    <%= friend.followee.first_name %>
    <%= friend.followee.last_name %>
    <%= friend.followee.username %>
    <a href="#">nudge?</a>
    <p>x</p>
  <% end %>
</div>

<%# REQUESTS %>
<div>
  <% @myrequests.each do |request| %>
