<% if current_user %>
  <div class="footer">
    <a href="/posts"><%= image_tag "logo.svg" %> </a>
     <div class="icon-link-container">
  <%= link_to friendships_path, class: "" do %>
    <i class="fa-solid fa-user-group"></i>
  <% end %>

</div>
      <%= link_to charts_path, class: "" do %>
      <i class="fa-solid fa-chart-simple"></i>
       <% end %>
    <% if current_user.photo.attached? %>
      <a href="/users/<%=current_user.id%>"><%= image_tag current_user.photo %></a>
    <% else %>
     <a href="/users/<%=current_user.id%>"> <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/5/59/User-avatar.svg/2048px-User-avatar.svg.png" alt="avatar" ></a>
    <% end %>
  </div>
  <div style="height: 70px;"></div>
<% end %>
